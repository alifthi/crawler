{
      "/wiki/%D9%88%DB%8C%DA%A9%DB%8C%E2%80%8C%D9%BE%D8%AF%DB%8C%D8%A7:%D8%AE%D9%88%D8%B4_%D8%A2%D9%85%D8%AF%DB%8C%D8%AF\n": {
            "corpus": "\u062e\u0648\u0634\u200c\u0622\u0645\u062f\u06cc\u062f \u2022 \n\u0635\u0641\u062d\u0647\u0654 \u062a\u0645\u0631\u06cc\u0646 \u2022 \n\u062e\u0648\u062f\u0622\u0645\u0648\u0632 \u2022 \n\u0641\u06cc\u0644\u0645\u200c\u0647\u0627\u06cc \u0622\u0645\u0648\u0632\u0634\u06cc \u2022 \n\u0622\u0645\u0648\u0632\u0634 \u0633\u0631\u06cc\u0639 \u2022 \n\u067e\u0631\u0633\u0634\u200c\u0647\u0627\u06cc \u0631\u0627\u06cc\u062c \u2022 \n\u0648\u0627\u0698\u0647\u200c\u0646\u0627\u0645\u0647 \u2022 \n\u0631\u0627\u0647\u0646\u0645\u0627 \u2022 \n\u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0631\u0627\u0647\u0646\u0645\u0627\u06cc\u06cc \u2022 \n\u0642\u0647\u0648\u0647\u200c\u062e\u0627\u0646\u0647 \u2022 \n\u067e\u0631\u0633\u0634\u200c\u0647\u0627 \u2022 \n\u062a\u0645\u0627\u0633 \u0628\u0627 \u0645\u0627\n\n\u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0628\u0627 \u0647\u0645\u06a9\u0627\u0631\u06cc \u062e\u0648\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u0634 \u0646\u0648\u0634\u062a\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f. \u0627\u06cc\u0646 \u0648\u0628\u06af\u0627\u0647 \u06cc\u06a9 \u0648\u06cc\u06a9\u06cc \u0627\u0633\u062a\u060c \u06cc\u0639\u0646\u06cc \u0647\u0631 \u06a9\u0633\u06cc\u060c \u0627\u0632 \u062c\u0645\u0644\u0647 \u062e\u0648\u062f \u0634\u0645\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0627 \u06a9\u0644\u06cc\u06a9 \u06a9\u0631\u062f\u0646 \u0631\u0648\u06cc \u067e\u06cc\u0648\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0647 \u062f\u0631 \u0647\u0645\u0647\u0654 \u0645\u0642\u0627\u0644\u0627\u062a \u0645\u06cc\u200c\u0622\u06cc\u062f\u060c \u0622\u0646 \u0631\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u06cc\u062f.\n\n\u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0632\u06cc\u0627\u062f\u06cc \u062f\u0631\u0628\u0627\u0631\u0647\u0654 \u0627\u0646\u0648\u0627\u0639 \u0648 \u0627\u0642\u0633\u0627\u0645 \u0645\u0648\u0636\u0648\u0639\u0627\u062a \u062f\u0627\u0631\u062f. \u0628\u0631\u0627\u06cc \u0622\u0632\u0645\u0627\u06cc\u0634 \u0628\u0647 \u0635\u0641\u062d\u0647\u0654 \u0627\u0635\u0644\u06cc \u0628\u0631\u0648\u06cc\u062f\u060c \u0645\u0648\u0636\u0648\u0639\u06cc \u0631\u0627 \u06a9\u0647 \u0628\u0631\u0627\u06cc\u062a\u0627\u0646 \u062c\u0630\u0627\u0628 \u0627\u0633\u062a \u0627\u0646\u062a\u062e\u0627\u0628 \u0648 \u0634\u0631\u0648\u0639 \u0628\u0647 \u06a9\u0646\u062f\u0648\u06a9\u0627\u0648 \u06a9\u0646\u06cc\u062f. \u062f\u0631 \u0628\u0627\u0644\u0627\u06cc \u0647\u0631 \u0635\u0641\u062d\u0647 \u0646\u06cc\u0632 \u0642\u0633\u0645\u062a\u06cc \u0628\u0631\u0627\u06cc \u062c\u0633\u062a\u200c\u0648\u062c\u0648 \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f \u062a\u0627 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0631\u0627 \u0628\u0647\u200c\u0633\u0631\u0639\u062a \u067e\u06cc\u062f\u0627 \u06a9\u0646\u06cc\u062f.\n\n\u0627\u06af\u0631 \u0686\u06cc\u0632\u06cc \u0631\u0627 \u062e\u0648\u0627\u0646\u062f\u06cc\u062f \u0648 \u0628\u0647\u200c\u0648\u0627\u0642\u0639 \u0628\u0631\u0627\u06cc\u062a\u0627\u0646 \u062c\u0627\u0644\u0628 \u0628\u0648\u062f\u060c \u06cc\u0627\u062f\u062f\u0627\u0634\u062a\u06cc \u062f\u0631 \u0642\u0633\u0645\u062a \u0635\u0641\u062d\u0647\u0654 \u0628\u062d\u062b \u0645\u0642\u0627\u0644\u0647 \u0628\u0646\u0648\u06cc\u0633\u06cc\u062f. \u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0627\u0631\u060c \u0627\u0648\u0644 \u067e\u06cc\u0648\u0646\u062f \u0628\u062d\u062b \u0631\u0627 \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f (\u062f\u0631 \u0646\u0648\u0627\u0631 \u0628\u0627\u0644\u0627\u06cc \u0635\u0641\u062d\u0647 \u0628\u0647 \u062f\u0646\u0628\u0627\u0644 \u0622\u0646 \u0628\u06af\u0631\u062f\u06cc\u062f) \u062a\u0627 \u0628\u0647 \u0635\u0641\u062d\u0647\u0654 \u0628\u062d\u062b \u0628\u0631\u0648\u06cc\u062f. \u0633\u067e\u0633 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0631\u0627 \u062f\u0631 \u0635\u0641\u062d\u0647\u0654 \u0628\u062d\u062b \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f. \u0622\u06af\u0627\u0647\u06cc \u0627\u0632 \u0646\u0638\u0631 \u0634\u0645\u0627 \u0645\u0627\u06cc\u0647\u0654 \u062e\u0648\u0634\u062d\u0627\u0644\u06cc \u0645\u0627\u0633\u062a.\n\n\u0627\u06af\u0631 \u0645\u0627 \u0628\u0647 \u0645\u0648\u0636\u0648\u0639\u06cc \u0646\u067e\u0631\u062f\u0627\u062e\u062a\u0647\u200c\u0627\u06cc\u0645 \u06cc\u0627 \u0628\u0631\u0627\u06cc \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u0646 \u0686\u06cc\u0632\u06cc \u062f\u0631\u062f\u0633\u0631 \u062f\u0627\u0631\u06cc\u062f\u060c \u062f\u0631 \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0631\u0627\u0647\u0646\u0645\u0627\u06cc\u06cc \u0628\u067e\u0631\u0633\u06cc\u062f \u06cc\u0627 \u0645\u0648\u0636\u0648\u0639 \u0631\u0627 \u0628\u0647 \u0641\u0647\u0631\u0633\u062a \u0645\u0642\u0627\u0644\u0627\u062a \u062f\u0631\u062e\u0648\u0627\u0633\u062a\u06cc \u0627\u0636\u0627\u0641\u0647 \u06a9\u0646\u06cc\u062f.\n\n\u0631\u062c\u0648\u0639 \u0634\u0648\u062f \u0628\u0647: \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627:\u0627\u06a9\u062a\u0634\u0627\u0641\n\n\u0647\u0631 \u06a9\u0633\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0635\u0641\u062d\u0627\u062a \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0631\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f. (\u062d\u062a\u06cc \u0627\u06cc\u0646 \u0635\u0641\u062d\u0647 \u0631\u0627!) \u06a9\u0627\u0641\u06cc \u0627\u0633\u062a \u067e\u06cc\u0648\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0631\u0627 \u062f\u0631 \u0628\u0627\u0644\u0627 \u06cc\u0627 \u067e\u0627\u06cc\u06cc\u0646 \u0635\u0641\u062d\u0647\u200c\u0627\u06cc \u06a9\u0647 \u0641\u06a9\u0631 \u0645\u06cc\u200c\u06a9\u0646\u06cc\u062f \u0646\u06cc\u0627\u0632\u0645\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0627\u0633\u062a \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f. \u0635\u0644\u0627\u062d\u06cc\u062a \u062e\u0627\u0635\u06cc \u0644\u0627\u0632\u0645 \u0646\u06cc\u0633\u062a\u060c \u062d\u062a\u06cc \u0644\u0627\u0632\u0645 \u0646\u06cc\u0633\u062a \u0628\u0647 \u0633\u06cc\u0633\u062a\u0645  \u0648\u0627\u0631\u062f \u0634\u0648\u06cc\u062f. \u0628\u0631\u0627\u06cc \u062a\u0645\u0631\u06cc\u0646 \u0627\u06cc\u0646 \u06a9\u0627\u0631\u060c \u0628\u062f\u0648\u0646 \u0631\u06cc\u0633\u06a9 \u062e\u0631\u0627\u0628 \u06a9\u0631\u062f\u0646 \u06cc\u06a9 \u0645\u0642\u0627\u0644\u0647\u0654 \u0648\u0627\u0642\u0639\u06cc \u0633\u0631\u06cc \u0628\u0647 \u0635\u0641\u062d\u0647 \u062a\u0645\u0631\u06cc\u0646 \u0628\u0632\u0646\u06cc\u062f. \u062c\u0627\u06cc\u06cc \u06a9\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0647\u0631 \u0686\u0642\u062f\u0631 \u062f\u0644\u062a\u0627\u0646 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u062a\u0645\u0631\u06cc\u0646 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u06cc\u062f. \u0628\u0631\u0627\u06cc \u062a\u0645\u0631\u06cc\u0646 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06cc\u06a9 \u0635\u0641\u062d\u0647\u0654 \u0645\u0648\u062c\u0648\u062f \u0645\u062b\u0644 \u0647\u0645\u06cc\u0646 \u0635\u0641\u062d\u0647\u060c \u0622\u0646 \u0631\u0627 \u062f\u0631 \u00ab\u0635\u0641\u062d\u0647 \u062a\u0645\u0631\u06cc\u0646\u00bb \u06a9\u067e\u06cc \u06a9\u0646\u06cc\u062f. \u0627\u06af\u0631 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u06cc\u062f \u0628\u06cc\u0634\u062a\u0631 \u0628\u062f\u0627\u0646\u06cc\u062f\u060c \u062e\u0648\u062f\u0622\u0645\u0648\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0648 \u06a9\u0627\u0631\u062a \u0631\u0627\u0647\u0646\u0645\u0627 \u0631\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f \u062a\u0627 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0636\u0631\u0648\u0631\u06cc \u0628\u0631\u0627\u06cc \u0627\u0639\u0636\u0627\u06cc \u0627\u06cc\u0646 \u067e\u0631\u0648\u0698\u0647 \u0631\u0627 \u06cc\u0627\u062f \u0628\u06af\u06cc\u0631\u06cc\u062f.\n\n\u06cc\u06a9 \u0631\u0627\u0647 \u0633\u0627\u062f\u0647 \u0628\u0631\u0627\u06cc \u06a9\u0645\u06a9\u060c \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0627\u0633\u062a\u060c \u062f\u0631\u0633\u062a \u0645\u0627\u0646\u0646\u062f \u0647\u0631 \u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u062f\u06cc\u06af\u0631\u061b \u0628\u0627 \u0627\u06cc\u0646 \u062a\u0641\u0627\u0648\u062a \u06a9\u0647 \u0647\u0631 \u0648\u0642\u062a \u0628\u0647 \u0627\u06cc\u0631\u0627\u062f\u06cc \u0628\u0631\u062e\u0648\u0631\u062f\u06cc\u062f -\u0645\u062b\u0644\u0627\u064b \u0628\u0647 \u06cc\u06a9 \u063a\u0644\u0637 \u0627\u0645\u0644\u0627\u06cc\u06cc \u06cc\u0627 \u06cc\u06a9 \u062c\u0645\u0644\u0647\u0654 \u0646\u0627\u0645\u0641\u0647\u0648\u0645- \u067e\u06cc\u0648\u0646\u062f \u00ab\u0648\u06cc\u0631\u0627\u06cc\u0634 \u0627\u06cc\u0646 \u0635\u0641\u062d\u0647\u00bb \u0631\u0627 \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f \u0648 \u062f\u0631\u0633\u062a\u0634 \u06a9\u0646\u06cc\u062f. \u062f\u0631 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0635\u0641\u062d\u0647\u200c\u0647\u0627 \u062c\u0633\u0648\u0631 \u0628\u0627\u0634\u06cc\u062f. \u0647\u0631\u06af\u0648\u0646\u0647 \u0628\u0647\u0628\u0648\u062f\u06cc \u0631\u0627 \u0627\u0639\u0645\u0627\u0644 \u06a9\u0646\u06cc\u062f. \u0632\u06cc\u0627\u062f \u0646\u06af\u0631\u0627\u0646 \u0627\u0634\u062a\u0628\u0627\u0647 \u06a9\u0631\u062f\u0646 \u0646\u0628\u0627\u0634\u06cc\u062f. \u0627\u0634\u062a\u0628\u0627\u0647\u0627\u062a \u06a9\u0648\u0686\u06a9 \u0631\u0627 \u0647\u0645\u06cc\u0634\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0639\u062f\u062a\u0631 \u0627\u0635\u0644\u0627\u062d \u06a9\u0646\u06cc\u062f \u06cc\u0627 \u062f\u06cc\u06af\u0631\u0627\u0646 \u0627\u0635\u0644\u0627\u062d \u06a9\u0646\u0646\u062f. \u0634\u0627\u06cc\u062f \u0627\u06cc\u0646 \u0645\u0633\u0626\u0644\u0647 \u06a9\u0645\u06cc \u062a\u0631\u0633\u0646\u0627\u06a9 \u0628\u0647 \u0646\u0638\u0631 \u0628\u0631\u0633\u062f. \u0628\u0631\u0627\u06cc \u062a\u0648\u0636\u06cc\u062d \u062f\u0644\u06cc\u0644 \u0639\u0645\u0644\u06cc \u0628\u0648\u062f\u0646 \u0627\u06cc\u0646 \u0633\u06cc\u0633\u062a\u0645\u060c \u067e\u0627\u0633\u062e \u0628\u0647 \u067e\u0631\u0633\u0634\u200c\u0647\u0627\u06cc \u0631\u0627\u06cc\u062c \u0631\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f. \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0628\u0627 \u0648\u062c\u0648\u062f\u0650 \u062f\u0627\u0634\u062a\u0646 \u062a\u0639\u062f\u0627\u062f \u0632\u06cc\u0627\u062f\u06cc \u0645\u0642\u0627\u0644\u0647\u060c \u0647\u0645\u0686\u0646\u0627\u0646 \u0628\u0647 \u062e\u0627\u0637\u0631 \u0627\u0636\u0627\u0641\u0647 \u0634\u062f\u0646 \u0645\u0642\u0627\u0644\u0627\u062a \u062c\u062f\u06cc\u062f\u06cc \u06a9\u0647 \u06a9\u0633\u0627\u0646\u06cc \u0645\u062b\u0644 \u0634\u0645\u0627 \u0645\u06cc\u200c\u0646\u0648\u06cc\u0633\u0646\u062f\u060c \u062f\u0631 \u062d\u0627\u0644 \u0631\u0634\u062f \u0627\u0633\u062a. \u0634\u0645\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0645\u0642\u0627\u0644\u0647\u200c\u0627\u06cc \u062a\u0627\u0632\u0647 \u0631\u0627 \u0634\u0631\u0648\u0639 \u06a9\u0646\u06cc\u062f \u06cc\u0627 \u06cc\u06a9\u06cc \u0627\u0632 \u0645\u0642\u0627\u0644\u0627\u062a \u0645\u0648\u062c\u0648\u062f \u0631\u0627 \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0631\u062f\u0647\u060c \u0628\u062e\u0634\u06cc \u062c\u062f\u06cc\u062f\u06cc \u0628\u0647 \u0622\u0646 \u0627\u0636\u0627\u0641\u0647 \u06a9\u0646\u06cc\u062f.\n\n\u0628\u0647\u062a\u0631 \u0627\u0633\u062a \u0646\u06af\u0627\u0647\u06cc \u0628\u0647 \u0633\u06cc\u0627\u0633\u062a\u200c\u0647\u0627 \u0648 \u062a\u0648\u0635\u06cc\u0647\u200c\u0647\u0627\u06cc \u0645\u0627 \u0628\u06cc\u0627\u0646\u062f\u0627\u0632\u06cc\u062f. \u0628\u0647 \u062e\u0635\u0648\u0635 \u0633\u06cc\u0627\u0633\u062a \u062f\u06cc\u062f\u06af\u0627\u0647 \u0628\u06cc\u200c\u0637\u0631\u0641 \u06a9\u0647 \u0628\u0647 \u0627\u06cc\u0646 \u0645\u0639\u0646\u0627\u0633\u062a \u06a9\u0647 \u0645\u0642\u0627\u0644\u0627\u062a \u0646\u0628\u0627\u06cc\u062f \u062c\u0627\u0646\u0628\u062f\u0627\u0631\u0627\u0646\u0647 \u0628\u0627\u0634\u0646\u062f \u0648 \u0628\u0627\u06cc\u062f \u062f\u06cc\u062f\u06af\u0627\u0647\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u062f\u0631\u0628\u0627\u0631\u0647\u0654 \u0645\u0648\u0636\u0648\u0639 \u0631\u0627 \u0645\u0646\u0635\u0641\u0627\u0646\u0647 \u0648 \u062f\u0644\u0633\u0648\u0632\u0627\u0646\u0647 \u0628\u06cc\u0627\u0646 \u06a9\u0646\u0646\u062f. \u0627\u06af\u0631 \u0645\u062a\u0648\u062c\u0647 \u0634\u062f\u06cc\u062f \u06a9\u0647 \u062a\u063a\u06cc\u06cc\u0631\u0627\u062a \u0634\u0645\u0627 \u0631\u0627 \u0627\u0635\u0644\u0627\u062d \u06cc\u0627 \u062d\u0630\u0641 \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u0628\u0631\u0627\u06cc \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f\u0646 \u0639\u0644\u062a \u062a\u0627\u0631\u06cc\u062e\u0686\u0647\u0654 \u0635\u0641\u062d\u0647\u060c \u0635\u0641\u062d\u0647\u0654 \u0628\u062d\u062b \u062e\u0648\u062f\u062a\u0627\u0646 \u0648 \u0635\u0641\u062d\u0647\u0654 \u0628\u062d\u062b \u0645\u0642\u0627\u0644\u0647 \u0631\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f. \u062a\u0645\u0627\u0645 \u0645\u0634\u0627\u0631\u06a9\u062a\u200c\u0647\u0627 \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u062a\u062d\u062a \u0627\u062c\u0627\u0632\u0647\u200c\u0646\u0627\u0645\u0647\u0654 \u0645\u0633\u062a\u0646\u062f\u0627\u062a \u0622\u0632\u0627\u062f \u06af\u0646\u0648 (GFDL) \u0645\u0646\u062a\u0634\u0631 \u0645\u06cc\u200c\u0634\u0648\u0646\u062f. \u0627\u06cc\u0646 \u0627\u062c\u0627\u0632\u0647\u200c\u0646\u0627\u0645\u0647 \u0642\u0627\u0628\u0644\u06cc\u062a \u062a\u0648\u0632\u06cc\u0639 \u0622\u0632\u0627\u062f \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0631\u0627 \u0628\u0631\u0627\u06cc \u0647\u0645\u06cc\u0634\u0647 \u062a\u0636\u0645\u06cc\u0646 \u0645\u06cc\u200c\u06a9\u0646\u062f (\u0628\u0631\u0627\u06cc \u0627\u0637\u0644\u0627\u0639 \u0628\u06cc\u0634\u062a\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627:\u062d\u0642\u0648\u0642 \u062a\u06a9\u062b\u06cc\u0631 \u0631\u0627 \u0628\u0628\u0646\u06cc\u062f).\n\n\u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u0646\u062f \u0627\u0645\u0627 \u0627\u06af\u0631 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u06cc\u062f \u062f\u0627\u0626\u0645 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u06cc\u062f\u060c \u062f\u0631\u0633\u062a \u06a9\u0631\u062f\u0646 \u062d\u0633\u0627\u0628 \u0645\u0632\u0627\u06cc\u0627\u06cc\u06cc \u062f\u0627\u0631\u062f. \u0627\u06af\u0631 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u06cc\u062f \u0639\u0636\u0648 \u0634\u0648\u06cc\u062f \u0628\u0631\u0627\u06cc \u062e\u0648\u062f\u062a\u0627\u0646 \u062d\u0633\u0627\u0628 \u0648 \u0646\u0627\u0645 \u06a9\u0627\u0631\u0628\u0631\u06cc \u0627\u06cc\u062c\u0627\u062f \u06a9\u0646\u06cc\u062f.\n\n\u0627\u06cc\u0646 \u06cc\u06a9 \u0645\u0642\u0627\u0644\u0647\u0654 \u062e\u0631\u062f  \u0645\u0631\u062a\u0628\u0637 \u0628\u0627 \u0628\u0646\u06cc\u0627\u062f \u0648\u06cc\u06a9\u06cc\u200c\u0645\u062f\u06cc\u0627 \u0627\u0633\u062a. \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u06cc\u062f \u0628\u0627 \u06af\u0633\u062a\u0631\u0634 \u0622\u0646 \u0628\u0647 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u06a9\u0645\u06a9 \u06a9\u0646\u06cc\u062f.\n",
            "header": "\u06af\u0634\u062a\u200c\u0648\u06af\u0630\u0627\u0631 \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627\n\u0648\u06cc\u0631\u0627\u06cc\u0634\n\u0628\u0647 \u0645\u0627 \u0628\u067e\u06cc\u0648\u0646\u062f\u06cc\u062f\n\u0645\u0631\u0627\u062c\u0639\u0627\u062a\n\u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0639\u0645\u0648\u0645\u06cc \u0648 \u0631\u0627\u0647\u0646\u0645\u0627\u0647\u0627\n\u0634\u0647\u0631\u0648\u0646\u062f\u06cc \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627\n\u06af\u0648\u0634\u0647\u0654 \u0622\u0645\u0648\u0632\u0634\u06cc\n\u06a9\u0646\u062f\u0648\u06a9\u0627\u0648\u0647\u0627\u06cc \u0639\u0645\u06cc\u0642\u200c\u062a\u0631\n\u0627\u062c\u062a\u0645\u0627\u0639 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627\n",
            "imageLinks": "//upload.wikimedia.org/wikipedia/commons/thumb/b/bd/Ambox_current_red_Asia_Australia.svg/42px-Ambox_current_red_Asia_Australia.svg.png\n//upload.wikimedia.org/wikipedia/commons/thumb/8/8b/Wikimedia-logo_black.svg/30px-Wikimedia-logo_black.svg.png\n//fa.wikipedia.org/wiki/Special:CentralAutoLogin/start?type=1x1\n",
            "flag": false
      },
      "/wiki/%D9%88%DB%8C%DA%A9%DB%8C%E2%80%8C%D9%BE%D8%AF%DB%8C%D8%A7:%D8%AF%D8%A7%D9%86%D8%B4%D9%86%D8%A7%D9%85%D9%87%D9%94_%D8%A2%D8%B2%D8%A7%D8%AF\n": {
            "corpus": "\u0632\u06cc\u0631 \u0639\u0646\u0648\u0627\u0646 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627\u060c \u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0622\u0632\u0627\u062f \u06a9\u0647 \u0647\u0631 \u06a9\u0633\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0622\u0646 \u0631\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f \u0627\u0633\u062a.\n\n\u0641\u0631\u0647\u0646\u06af \u0645\u0647\u0645 \u0628\u0639\u062f\u06cc \u06a9\u0647 \u0686\u0627\u067e \u0634\u062f \u06cc\u06a9 \u0641\u0631\u0647\u0646\u06af \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \u0641\u0631\u0627\u0646\u0633\u0648\u06cc \u0627\u062b\u0631 \u062c\u0627\u0646 (\u06cc\u0627 \u062c\u0650\u0647\u0627\u0646) \u067e\u0627\u0644\u0633\u06af\u0631\u06cc\u0648 \u062f\u0631 \u06f1\u06f5\u06f3\u06f0 \u0628\u0648\u062f [...] \u0648 \u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u0628\u0647 \u062c\u0627 \u0645\u0627\u0646\u062f\u0647\u200c\u0627\u0633\u062a \u06a9\u0647 \u0646\u0634\u0627\u0646 \u0645\u06cc\u200c\u062f\u0647\u062f \u0627\u0648 \u0648 \u0647\u0645\u06a9\u0627\u0631\u0634 \u0647\u0645\u0627\u0647\u0646\u06af \u06a9\u0631\u062f\u0646\u062f \u06a9\u0647 \u0647\u06cc\u0686 \u0631\u0648\u0646\u0648\u0634\u062a\u06cc \u0631\u0627 \u0646\u0628\u0627\u06cc\u062f \u0628\u062f\u0648\u0646 \u0627\u062c\u0627\u0632\u0647\u0654 \u0627\u0648 \u0628\u0647 \u0641\u0631\u0648\u0634 \u0631\u0633\u0627\u0646\u0646\u062f\u060c\n\n\u0648\u0627\u0698\u0647\u0654 \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \"free\" \u062f\u0627\u0631\u0627\u06cc \u0686\u0646\u062f\u06cc\u0646 \u0645\u0639\u0646\u06cc \u0627\u0633\u062a. \u0648\u0627\u0698\u0647\u0654 \"Free\" \u062f\u0631 \u0639\u0628\u0627\u0631\u062a \u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0622\u0632\u0627\u062f\u00bb \u0627\u0628\u062a\u062f\u0627 \u0648 \u067e\u06cc\u0634 \u0627\u0632 \u0647\u0631 \u0686\u06cc\u0632 \u0628\u0647 \u0634\u0631\u0627\u06cc\u0637 \u0645\u062c\u0648\u0632 \u0645\u062d\u062a\u0648\u0627\u06cc \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0631\u062f. \u0645\u062a\u0648\u0646 \u0627\u0639\u0637\u0627\u06cc\u06cc \u0628\u0647 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u062a\u062d\u062a \u0645\u062c\u0648\u0632\u0647\u0627\u06cc \u06a9\u067e\u06cc\u200c\u0644\u0641\u062a \u0645\u062d\u062a\u0648\u0627\u06cc \u0622\u0632\u0627\u062f \u0645\u0627\u0646\u0646\u062f \u0645\u062c\u0648\u0632 \u06a9\u0631\u06cc\u062a\u06cc\u0648 \u06a9\u0627\u0645\u0646\u0632 \u0627\u0633\u0646\u0627\u062f-\u0627\u0634\u062a\u0631\u0627\u06a9\u200c\u06cc\u06a9\u0633\u0627\u0646 (CC-BY-SA) \u0648 \u0645\u062c\u0648\u0632 \u0645\u0633\u062a\u0646\u062f\u0627\u062a \u0622\u0632\u0627\u062f \u06af\u0646\u0648 (GFDL) \u0647\u0633\u062a\u0646\u062f. \u0645\u0641\u0647\u0648\u0645 \u0627\u0635\u0644\u06cc \u0627\u06cc\u0646 \u0627\u0645\u0631 \u0622\u0646 \u0627\u0633\u062a \u06a9\u0647 \u062d\u0642\u0648\u0642 \u0627\u06cc\u0646 \u0645\u062a\u0646\u200c\u0647\u0627 \u0645\u062a\u0639\u0644\u0642 \u0628\u0647 \u0646\u0648\u06cc\u0633\u0646\u062f\u06af\u0627\u0646 \u0622\u0646\u200c\u0647\u0627\u0633\u062a\u060c \u0627\u0645\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0645\u062a\u0646\u200c\u0647\u0627 \u0631\u0627 \u0622\u0632\u0627\u062f\u0627\u0646\u0647 \u062e\u0648\u0627\u0646\u062f\u060c \u062a\u0648\u0632\u06cc\u0639 \u06a9\u0631\u062f \u06cc\u0627 \u062a\u063a\u06cc\u06cc\u0631 \u062f\u0627\u062f\u060c \u0628\u0647 \u0634\u0631\u0637 \u0627\u06cc\u0646\u06a9\u0647 \u0646\u0633\u062e\u0647\u200c\u0647\u0627\u06cc \u062a\u063a\u06cc\u06cc\u0631\u06cc\u0627\u0641\u062a\u0647 \u0628\u0627 \u0645\u062c\u0648\u0632 \u06cc\u06a9\u0633\u0627\u0646\u06cc \u062a\u0648\u0632\u06cc\u0639 \u0634\u0648\u0646\u062f. \u0628\u0631\u0627\u06cc \u062f\u0627\u0646\u0633\u062a\u0646 \u0634\u0631\u0627\u06cc\u0637 \u062f\u0642\u06cc\u0642 \u062d\u0642\u0648\u0642\u06cc \u0645\u062a\u0646 \u06a9\u0627\u0645\u0644 \u0645\u062c\u0648\u0632 \u0631\u0627 \u0628\u062e\u0648\u0627\u0646\u06cc\u062f.\n\n\u0647\u062f\u0641 \u0627\u0632 \u0627\u06cc\u0646 \u0631\u0648\u0634 \u0627\u0639\u0637\u0627\u06cc \u0645\u062c\u0648\u0632 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u0645\u0637\u0627\u0644\u0628 \u0645\u0631\u062c\u0639 \u0628\u0647 \u0635\u0648\u0631\u062a \u0622\u0632\u0627\u062f \u0628\u0631\u0627\u06cc \u0647\u0645\u0647\u0654 \u0627\u0641\u0631\u0627\u062f \u062f\u0631 \u062f\u0633\u062a\u0631\u0633 \u0628\u0627\u0634\u0646\u062f\u2014\u0645\u062c\u0645\u0648\u0639\u0647\u200c\u0627\u06cc \u0627\u0632 \u062f\u0627\u0646\u0634 \u0628\u0634\u0631\u06cc \u06a9\u0647 \u0646\u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0622\u0646 \u0631\u0627 \u0628\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647\u0654 \u0645\u062d\u062f\u0648\u062f\u06a9\u0646\u0646\u062f\u0647 \u0627\u0632 \u062d\u0642 \u062a\u06a9\u062b\u06cc\u0631 \u0645\u062d\u062f\u0648\u062f \u06cc\u0627 \u06a9\u0646\u062a\u0631\u0644 \u06a9\u0631\u062f \u0648 \u0628\u0627 \u0645\u0639\u0642\u0648\u0644\u200c\u062a\u0631\u06cc\u0646 \u0634\u0631\u0627\u06cc\u0637 \u0628\u062a\u0648\u0627\u0646 \u0627\u0632 \u0622\u0646 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u06a9\u0631\u062f \u0648 \u0628\u0647 \u0627\u0634\u062a\u0631\u0627\u06a9\u0634 \u06af\u0630\u0627\u0634\u062a\u060c \u0628\u062f\u0648\u0646 \u0647\u06cc\u0686 \u0645\u0646\u0639\u06cc\u061b \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u06af\u0631\u0627\u06cc\u0634\u06cc \u0631\u0627 \u06a9\u0647 \u0646\u0632\u062f\u06cc\u06a9 \u0628\u0647 \u0646\u06cc\u0645\u200c\u0647\u0632\u0627\u0631\u0647\u0654 \u06af\u0630\u0634\u062a\u0647 \u062a\u0648\u0633\u0637 \u0622\u0642\u0627\u06cc \u062c\u0627\u0646 \u067e\u0627\u0644\u0633\u06af\u0631\u06cc\u0648 \u0622\u063a\u0627\u0632 \u06af\u0634\u062a \u0645\u0639\u06a9\u0648\u0633 \u0645\u06cc\u200c\u06a9\u0646\u062f.\n\n\u0628\u0627\u0631\u06af\u0630\u0627\u0631\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a\u06cc \u06a9\u0647 \u0634\u0631\u0627\u06cc\u0637 \u062d\u0642 \u0646\u0634\u0631 \u0622\u0646\u200c\u0647\u0627 \u0628\u0627 \u062c\u06cc\u200c\u0627\u0641\u200c\u062f\u06cc\u200c\u0627\u0644 \u0648 CC-BY-SA \u0633\u0627\u0632\u06af\u0627\u0631 \u0646\u0628\u0627\u0634\u062f \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0645\u062c\u0627\u0632 \u0646\u06cc\u0633\u062a \u0648 \u0628\u06cc\u200c\u062f\u0631\u0646\u06af \u062d\u0630\u0641 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u062a\u0646\u0647\u0627 \u0627\u0633\u062a\u062b\u0646\u0627\u06cc \u0627\u06cc\u0646 \u0642\u0627\u0646\u0648\u0646 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u062f\u0631 \u0628\u0631\u062e\u06cc \u0627\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627\u0647\u0627 \u0628\u0627\u0631\u06af\u0630\u0627\u0631\u06cc \u0628\u0631\u062e\u06cc \u0627\u0632 \u062a\u0635\u0627\u0648\u06cc\u0631 \u0648 \u067e\u0631\u0648\u0646\u062f\u0647\u200c\u0647\u0627\u06cc \u0631\u0633\u0627\u0646\u0647\u200c\u0627\u06cc \u062a\u062d\u062a \u0634\u0631\u0627\u06cc\u0637 \u00ab\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u0646\u0635\u0641\u0627\u0646\u0647\u00bb \u0645\u062c\u0627\u0632 \u062f\u0627\u0646\u0633\u062a\u0647 \u0634\u062f\u0647\u200c\u0627\u0633\u062a. \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627:\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u0646\u0635\u0641\u0627\u0646\u0647 \u0631\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f. (\u0628\u0631\u062e\u06cc \u0627\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0646\u200c\u0647\u0627 \u062f\u0631 \u0632\u0628\u0627\u0646\u200c\u0647\u0627\u06cc \u062f\u06cc\u06af\u0631\u060c \u0645\u0627\u0646\u0646\u062f \u0627\u0633\u067e\u0627\u0646\u06cc\u0627\u06cc\u06cc\u060c \u0627\u062c\u0627\u0632\u0647\u0654 \u00ab\u0627\u0633\u062a\u0641\u0627\u062f\u0647\u0654 \u0645\u0646\u0635\u0641\u0627\u0646\u0647\u00bb \u0631\u0627 \u0628\u0647 \u0647\u06cc\u0686 \u0648\u062c\u0647 \u0646\u0645\u06cc\u200c\u062f\u0647\u0646\u062f \u0648 \u0647\u06cc\u0686 \u0686\u06cc\u0632\u06cc \u062c\u0632 \u0645\u062d\u062a\u0648\u0627\u06cc \u0622\u0632\u0627\u062f \u0631\u0627 \u0646\u0645\u06cc\u200c\u067e\u0630\u06cc\u0631\u0646\u062f)\n\n\u0647\u0631 \u06a9\u0633\u06cc \u06a9\u0647 \u0628\u0647 \u062a\u0627\u0631 \u062c\u0647\u0627\u0646\u200c\u06af\u0633\u062a\u0631 \u062f\u0633\u062a\u0631\u0633\u06cc \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0645\u062d\u062a\u0648\u0627\u06cc \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0631\u0627 \u0627\u0632 \u0648\u0628\u06af\u0627\u0647 wikipedia.org \u0628\u062e\u0648\u0627\u0646\u062f \u0628\u062f\u0648\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u0647\u06cc\u0686 \u067e\u0648\u0644\u06cc \u0628\u067e\u0631\u062f\u0627\u0632\u062f. \u0627\u0645\u0627 \u0628\u0627 \u0627\u06cc\u0646 \u0648\u062c\u0648\u062f \u0627\u06cc\u0646 \u0628\u0647 \u0622\u0646 \u0645\u0639\u0646\u0627 \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u06a9\u0627\u0645\u0644\u0627\u064b \u063a\u06cc\u0631\u062a\u062c\u0627\u0631\u06cc \u0627\u0633\u062a. \u0631\u0648\u0646\u0648\u0634\u062a\u200c\u0647\u0627\u06cc \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0631\u0627 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0641\u0631\u0648\u062e\u062a. \u0645\u062c\u0648\u0632\u0647\u0627\u06cc CC-BY-SA \u0648 \u062c\u06cc\u200c\u0627\u0641\u200c\u062f\u06cc\u200c\u0627\u0644 \u0627\u062c\u0627\u0632\u0647 \u0645\u06cc\u200c\u062f\u0647\u0646\u062f \u062f\u0631 \u0645\u0642\u0627\u0628\u0644 \u0645\u062d\u062a\u0648\u0627\u06cc \u062a\u062d\u062a \u067e\u0648\u0634\u0634\u0634\u0627\u0646 \u067e\u0648\u0644 \u062f\u0631\u06cc\u0627\u0641\u062a \u0634\u0648\u062f. \u062f\u0631 \u062d\u0642\u06cc\u0642\u062a\u060c \u0628\u062e\u0634\u200c\u0647\u0627\u06cc\u06cc \u0627\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0631\u0648\u06cc \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627\u06cc \u0646\u0648\u0631\u06cc (\u0645\u0627\u0646\u0646\u062f \u062f\u06cc\u200c\u0648\u06cc\u200c\u062f\u06cc) \u0631\u0648\u0646\u0648\u0634\u062a\u200c\u0628\u0631\u062f\u0627\u0631\u06cc \u0634\u062f\u0647\u200c\u0627\u0646\u062f \u06cc\u0627 \u0631\u0648\u06cc \u06a9\u0627\u063a\u0630 \u0686\u0627\u067e \u0634\u062f\u0647\u200c\u0627\u0646\u062f\u060c \u0648 \u0627\u06cc\u0646 \u0631\u0648\u0646\u0648\u0634\u062a\u200c\u0647\u0627 \u0628\u0647 \u0635\u0648\u0631\u062a \u062a\u062c\u0627\u0631\u06cc \u062a\u0648\u0632\u06cc\u0639 \u0645\u06cc\u200c\u06af\u0631\u062f\u0646\u062f.\n\n\u0627\u0632 \u0627\u06cc\u0646 \u06af\u0630\u0634\u062a\u0647\u060c \u0628\u0627\u0631\u06af\u0630\u0627\u0631\u06cc \u0647\u0631 \u0646\u0648\u0639 \u0645\u062d\u062a\u0648\u0627\u06cc\u06cc \u06a9\u0647 \u0634\u0631\u0627\u06cc\u0637 \u062d\u0642 \u0646\u0634\u0631 \u0622\u0646 \u062a\u0646\u0647\u0627 \u0627\u062c\u0627\u0632\u0647\u0654 \u0627\u0633\u062a\u0641\u0627\u062f\u0647\u0654 \u063a\u06cc\u0631\u062a\u062c\u0627\u0631\u06cc \u0631\u0627 \u0628\u062f\u0647\u062f \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0645\u0645\u0646\u0648\u0639 \u0627\u0633\u062a\u060c \u0686\u0631\u0627\u06a9\u0647 \u0627\u06cc\u0646 \u0645\u0648\u0636\u0648\u0639 \u0628\u0627 \u0645\u0639\u0646\u0627\u06cc \u00ab\u0622\u0632\u0627\u062f\u06cc\u00bb \u0622\u0646\u0686\u0646\u0627\u0646 \u06a9\u0647 \u062f\u0631 CC-BY-SA \u06cc\u0627 \u062c\u06cc\u200c\u0627\u0641\u200c\u062f\u06cc\u200c\u0627\u0644 \u062a\u0639\u0631\u06cc\u0641 \u0634\u062f\u0647\u200c\u0627\u0633\u062a \u0633\u0627\u0632\u06af\u0627\u0631 \u0646\u06cc\u0633\u062a.\n\n\u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u06a9\u0647 \u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0622\u0646 \u0631\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u0646\u062f\u00bb \u0628\u0647 \u0627\u06cc\u0646 \u0645\u0639\u0646\u06cc \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0647\u0631 \u06a9\u0633\u06cc \u062d\u0642 \u062f\u0627\u0631\u062f \u0645\u0634\u0627\u0631\u06a9\u062a\u06cc \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f \u06a9\u0647 \u0627\u0632 \u062f\u06cc\u062f\u06af\u0627\u0647 \u062e\u0648\u062f\u0634 \u067e\u0634\u062a\u06cc\u0628\u0627\u0646\u06cc \u0645\u06cc\u200c\u06a9\u0646\u062f. \u0628\u0631\u062e\u06cc \u0648\u06cc\u0631\u0627\u06cc\u0634\u06af\u0631\u0627\u0646 \u0627\u06cc\u0646\u0637\u0648\u0631 \u0628\u0631\u062f\u0627\u0634\u062a \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u06a9\u0647 \u00ab\u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u0646\u062f\u00bb \u062f\u0631 \u0639\u0628\u0627\u0631\u062a \u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u06a9\u0647 \u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u0646\u062f\u00bb \u0646\u0648\u0639\u06cc \u06af\u0633\u062a\u0631\u0634 \u0645\u0639\u0646\u0627\u06cc \u00ab\u0622\u0632\u0627\u062f\u06cc\u00bb \u0627\u0633\u062a\u061b \u0627\u0645\u0627 \u0627\u06cc\u0646 \u062f\u0631\u0633\u062a \u0646\u06cc\u0633\u062a\u060c \u0686\u0631\u0627 \u06a9\u0647 \u0627\u06cc\u0646 \u062c\u0645\u0644\u0647 \u062f\u0648 \u0645\u0641\u0647\u0648\u0645 \u062c\u062f\u0627\u06af\u0627\u0646\u0647 \u0631\u0627 \u062f\u0631\u0628\u0631\u062f\u0627\u0631\u062f\u060c \u0647\u0645\u0627\u0646\u200c\u0637\u0648\u0631 \u06a9\u0647 \u0628\u0627\u0644\u0627 \u0647\u0645 \u06af\u0641\u062a\u0647 \u0634\u062f \u00ab\u0622\u0632\u0627\u062f\u00bb \u0628\u0647 \u0645\u062c\u0648\u0632 \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0631\u062f \u0627\u0645\u0627 \u00ab\u0647\u0631 \u06a9\u0633\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f\u00bb \u0628\u0647 \u0627\u06cc\u0646 \u0648\u0627\u0642\u0639\u06cc\u062a \u0628\u0631\u0645\u06cc\u200c\u06af\u0631\u062f\u062f \u06a9\u0647 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u06cc\u06a9 \u0648\u06cc\u06a9\u06cc \u0627\u0633\u062a\u060c \u0648\u0628\u200c\u06af\u0627\u0647\u06cc \u06a9\u0647 \u0647\u0631 \u06a9\u0633\u06cc \u0628\u0627 \u062f\u0627\u0634\u062a\u0646 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0647 \u0648\u0628 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u062f\u0631 \u0622\u0646 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f \u0628\u062f\u0648\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u0644\u0627\u0632\u0645 \u0628\u0627\u0634\u062f \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u062f\u0633\u062a\u0631\u0633\u06cc \u0646\u0645\u0627\u06cc\u062f. \u0628\u0627 \u0627\u06cc\u0646 \u0648\u062c\u0648\u062f \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u062a\u0644\u0627\u0634 \u062f\u0627\u0631\u062f \u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u0642\u0627\u0628\u0644 \u0627\u0639\u062a\u0645\u0627\u062f \u0628\u0627\u0634\u062f \u06a9\u0647 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0631\u0627 \u0628\u0627 \u062f\u06cc\u062f\u06af\u0627\u0647\u06cc \u0628\u06cc\u200c\u0637\u0631\u0641 \u0648 \u0627\u0632 \u0631\u0648\u06cc \u0645\u0646\u0627\u0628\u0639 \u062a\u0623\u06cc\u06cc\u062f\u067e\u0630\u06cc\u0631 \u0628\u0647 \u062e\u0648\u0627\u0646\u0646\u062f\u0647 \u0627\u0631\u0627\u0626\u0647 \u0645\u06cc\u200c\u06a9\u0646\u062f. \u062a\u0627 \u0632\u0645\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u0631\u0627\u0633\u062a\u0627\u06cc \u0627\u06cc\u0646 \u0647\u062f\u0641\u200c\u0647\u0627 \u0639\u0645\u0644 \u0634\u0648\u062f\u060c \u0647\u0631 \u06a9\u0633\u06cc \u0627\u062c\u0627\u0632\u0647 \u062f\u0627\u0631\u062f \u0628\u0627 \u062c\u0633\u0627\u0631\u062a \u062f\u0631 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f\u060c \u0627\u0645\u0627 \u0648\u06cc\u0631\u0627\u06cc\u0634\u200c\u0647\u0627\u06cc\u06cc \u06a9\u0647 \u0628\u0627 \u0627\u06cc\u0646 \u0642\u0648\u0627\u0639\u062f \u0633\u0627\u0632\u06af\u0627\u0631 \u0646\u0628\u0627\u0634\u0646\u062f \u0648\u0627\u06af\u0631\u062f\u0627\u0646\u06cc \u062e\u0648\u0627\u0647\u0646\u062f \u0634\u062f \u0648 \u062f\u0631 \u0645\u0648\u0627\u0631\u062f \u0634\u062f\u06cc\u062f \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u0648\u06cc\u0631\u0627\u06cc\u0634\u06af\u0631\u06cc \u06a9\u0647 \u0622\u0646 \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0631\u0627 \u0627\u0646\u062c\u0627\u0645 \u062f\u0627\u062f\u0647\u200c\u0627\u0633\u062a \u0642\u0637\u0639 \u062f\u0633\u062a\u0631\u0633\u06cc \u0634\u0648\u062f.\n\n\u06cc\u06a9 \u0645\u0639\u0646\u0627\u06cc \u0631\u0627\u06cc\u062c \u0645\u062d\u062a\u0648\u0627\u06cc \u0622\u0632\u0627\u062f \u0648 \u0646\u0631\u0645\u200c\u0627\u0641\u0632\u0627\u0631 \u0622\u0632\u0627\u062f \u00ab\u0622\u0632\u0627\u062f\u06cc \u0645\u0627\u0646\u0646\u062f \u0622\u0632\u0627\u062f\u06cc \u0628\u06cc\u0627\u0646\u060c \u0646\u0647 \u0646\u0648\u0634\u06cc\u062f\u0646\u06cc \u0631\u0627\u06cc\u06af\u0627\u0646\u00bb \u0627\u0633\u062a. \u0627\u0645\u0627 \u0647\u0645\u0686\u0646\u0627\u0646 \u0627\u06cc\u0646 \u0645\u0648\u0636\u0648\u0639 \u0628\u0647 \u0645\u062c\u0648\u0632 \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0627\u0634\u0627\u0631\u0647 \u062f\u0627\u0631\u062f. \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0628\u0647 \u0622\u0632\u0627\u062f\u06cc \u0628\u06cc\u0627\u0646 \u0627\u062d\u062a\u0631\u0627\u0645 \u0645\u06cc\u200c\u06af\u0630\u0627\u0631\u062f\u060c \u0627\u0645\u0627 \u0622\u0632\u0627\u062f\u06cc \u0628\u06cc\u0627\u0646 \u0627\u0632 \u0633\u06cc\u0627\u0633\u062a\u200c\u0647\u0627\u06cc \u0645\u062d\u062a\u0648\u0627\u06cc\u06cc \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627 \u0646\u06cc\u0633\u062a. \u0628\u0631\u0627\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0628\u06cc\u0634\u062a\u0631 \u062f\u0631 \u0627\u06cc\u0646 \u0632\u0645\u06cc\u0646\u0647\u060c \u0648\u06cc\u06a9\u06cc\u200c\u067e\u062f\u06cc\u0627:\u0622\u0632\u0627\u062f\u06cc \u0628\u06cc\u0627\u0646 \u0631\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f.\n\n",
            "header": "\u0645\u0639\u0646\u0627\u06cc \u0627\u0635\u0644\u06cc \u00ab\u0622\u0632\u0627\u062f\u00bb\n\u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0622\u0632\u0627\u062f\u00bb \u0628\u0647 \u0645\u0639\u0646\u06cc \u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0631\u0627\u06cc\u06af\u0627\u0646\u00bb \u0646\u06cc\u0633\u062a\n\u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0622\u0632\u0627\u062f\u00bb \u0628\u0647 \u0645\u0639\u0646\u06cc \u00ab\u0647\u0631 \u06a9\u0633\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0647\u0631\u0637\u0648\u0631 \u06a9\u0647 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u0648\u06cc\u0631\u0627\u06cc\u0634 \u06a9\u0646\u062f\u00bb \u0646\u06cc\u0633\u062a\n\u00ab\u062f\u0627\u0646\u0634\u0646\u0627\u0645\u0647\u0654 \u0622\u0632\u0627\u062f\u00bb \u0644\u0632\u0648\u0645\u0627\u064b \u0628\u0647 \u00ab\u0622\u0632\u0627\u062f\u06cc \u0628\u06cc\u0627\u0646\u00bb \u0627\u0634\u0627\u0631\u0647 \u0646\u062f\u0627\u0631\u062f\n\u062c\u0633\u062a\u0627\u0631\u0647\u0627\u06cc \u0648\u0627\u0628\u0633\u062a\u0647\n",
